{
    "name": "vscode-extension-webview-template",
    "displayName": "WebView Example",
    "description": "A vscode webview extension example.",
    "version": "0.0.2",
    "publisher": "leocll",
    "icon": "media/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/leocll/vscode-extension-webview-template.git"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:example.webview",
        "onCommand:example.helloWorld",
        "onView:leocll_example_activitybar.view1",
        "onView:leocll_example_activitybar.view2",
        "onView:leocll_example_activitybar.view3",
        "onView:leocll_example_panel.view1",
        "onView:leocll_example_panel.view2"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "leocll_example_activitybar",
                    "title": "Example",
                    "icon": "media/logo.png"
                }
            ],
            "panel": [
                {
                    "id": "leocll_example_panel",
                    "title": "Example",
                    "icon": "media/logo.png"
                }
            ]
        },
        "views": {
            "leocll_example_activitybar": [
                {
                    "id": "leocll_example_activitybar.view1",
                    "name": "Example View1",
                    "type": "webview",
                    "visibility": "hidden"
                },
                {
                    "id": "leocll_example_activitybar.view2",
                    "name": "Example View2",
                    "type": "webview"
                },
                {
                    "id": "leocll_example_activitybar.view3",
                    "name": "Example View3",
                    "type": "webview"
                }
            ],
            "leocll_example_panel": [
                {
                    "id": "leocll_example_panel.view1",
                    "name": "Example Panel View1",
                    "type": "webview"
                },
                {
                    "id": "leocll_example_panel.view2",
                    "name": "Example Panel View2",
                    "type": "webview",
                    "visibility": "collapsed"
                }
            ]
        },
        "commands": [
            {
                "command": "example.webview",
                "title": "Example: WebView"
            },
            {
                "command": "example.helloWorld",
                "title": "Example: Hello World"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "build": "npm run buildweb && vsce package",
        "buildext": "vsce package",
        "buildweb": "cd ./web && { npm run build;cd -;}",
        "startweb": "cd ./web && { npm run dev;cd -;}"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.50.0",
        "@typescript/server-harness": "^0.1.4",
        "eslint": "^5.13.0",
        "typescript": "^3.3.1"
    },
    "dependencies": {
        "htmlparser2": "^7.2.0",
        "request": "^2.88.2",
        "@leocll/vscode-webview": "file:./libs/vscode-webview"
    }
}
